@model IEnumerable<TuLista.Models.CompraActiva>

<table class="table table-hover table-responsive table-condensed">
    <tr>
        <th>Lista</th>
        <th></th>
    </tr>
    @foreach (var l in Model)
    {
        <tr>
            <td>@l.titulo</td>
            <td>

                <a href="/Listas/verDetallesDeLista?idLista=@l.idLista">Detalles</a>
            </td>
        </tr>
    }
</table>
